{
    "manifest_version": 2,
    "name": "search chrome history by date",
    "version": "1.0",
    "description": "a extention for Google Chrome that searchinng chrome history by date",
    "browser_action": {
        "default_popup": "./popup/popup.html"
    },
    "permissions": [
        "history",
        "background",
        "tabs"
    ],
    "background": {
        "scripts": ["./background/background.js"]
    }
}